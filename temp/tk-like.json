{
   "tk_x3_open_apk":{
      "name":"START_APK",
      "desc":"Open apk",
      "trigger":[
         
      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"am start -a android.intent.action.VIEW -d {link} com.zhiliaoapp.musically"
         }
      },
      "order":0,
      "type":"static",
      "preDelay":2000,
      "postDelay":16000,
      "maxLoop":10,
      "loop":0,
      "maxTry":1,
      "try":0,
      "timeout":5000,
      "next":[
         "tk_x3_like"
      ]
   },
   "tk_x3_like":{
      "name":"TK_LIVE_LIKE",
      "desc":"tiktok Live send like",
      "trigger":[
         
      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"\"input tap 324 521 & sleep 0.1; input tap 324 521\""
         }
      },
      "order":0,
      "type":"static",
      "preDelay":200,
      "postDelay":4000,
      "maxLoop":10,
      "loop":0,
      "maxTry":10,
      "try":0,
      "timeout":500,
      "next":[
         "tk_x3_pattern_like_next"
      ]
   },
