      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"am broadcast -a ADB_EDITOR_CODE --ei code 5"
         }
      },
      "order":0,
      "type":"static",
      "preDelay":500,
      "postDelay":2000,
      "maxLoop":10,
      "loop":0,
      "maxTry":10,
      "try":0,
      "timeout":5000,
      "next":[
         "tk03_send",
