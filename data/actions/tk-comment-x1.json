{
   "tk03_start":{
      "name":"START_APK",
      "desc":"Start apk",
      "trigger":[
         
      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"am start -a android.intent.action.VIEW -d {link} com.zhiliaoapp.musically"
         }
      },
      "order":0,
      "type":"static",
      "preDelay":1000,
      "postDelay":9500,
      "maxLoop":10,
      "loop":0,
      "maxTry":1,
      "try":0,
      "timeout":5000,
      "next":[
         "tk03_comment_icon","tk03_comment_icon","tk03_comment_icon","tk03_comment_icon","tk03_comment_icon","tk03_comment_icon","tk03_comment_icon","tk03_comment_icon","tk03_comment_icon"
      ]
   },
   "tk03_comment_icon":{
      "name":"PATTERN",
      "desc":"Find pattern send icon",
      "trigger":[
         {
            "type":"pattern",
            "pattern":"tk3_pattern_icon_comment",
            "condition":true,
            "cmd":{
               "action":"Screen",
               "devices":"{deviceId}",
               "data":{
                  "savePath":"{screen_path}"
               }
            },
            "result":{
               "x":-1,
               "y":-1,
               "w":-1,
               "h":-1,
               "wm":-1,
               "hm":-1
            }
         }
      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"input tap {wm} {hm}"
         }
      },
      "order":0,
      "type":"pattern",
      "preDelay":200,
      "postDelay":4000,
      "maxLoop":10,
      "loop":0,
      "maxTry":10,
      "try":0,
      "timeout":5000,
      "next":[
         "tk03_tap"
      ],
      "beforeLoop":"tk03_tap"
   },
   "tk03_tap":{
      "name":"CLICK",
      "desc":"CLICK",
      "trigger":[
         
      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"input tap 180 1459"
         }
      },
      "order":0,
      "type":"static",
      "preDelay":500,
      "postDelay":3000,
      "maxLoop":10,
      "loop":0,
      "maxTry":10,
      "try":0,
      "timeout":5000,
      "next":[
         "tk03_clear"
      ]
   },
   
