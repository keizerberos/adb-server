   "tk03_home":{
      "name":"HOME",
      "desc":"Go home",
      "trigger":[
         
      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"\"input keyevent 4 & sleep 1; input keyevent 3\""
         }
      },
