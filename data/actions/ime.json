{
   "ime_1":{
      "name":"ADB_COMMAND",
      "desc":"Enable IME",
      "trigger":[
         
      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"ime enable com.android.adbkeyboard/.AdbIME"
         }
      },
      "order":0,
      "type":"static",
      "preDelay":500,
