{
   "tk_follow_open":{
      "name":"START_APK",
      "desc":"Start apk",
      "trigger":[
         
      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"am start -a android.intent.action.VIEW -d {link} com.zhiliaoapp.musically"
         }
      },
      "order":0,
      "type":"static",
      "preDelay":1000,
      "postDelay":9500,
      "maxLoop":10,
      "loop":0,
      "maxTry":1,
      "try":0,
      "timeout":5000,
      "next":[
         "tk_follow_find",
         "tk_follow_find",
         "tk_follow_find",
         "tk_follow_find",
         "tk_follow_find",
         "tk_follow_end"
      ]
   },
   "tk_follow_find":{
      "name":"TK_FOLLOW",
      "desc":"tiktok follow live",
      "trigger":[
         {
            "type":"pattern",
            "pattern":"tk_pattern_pf_follow",
            "condition":true,
            "cmd":{
               "action":"Screen",
               "devices":"{deviceId}",
               "data":{
                  "savePath":"{screen_path}"
               }
            },
            "result":{
               "x":-1,
               "y":-1,
               "w":-1,
               "h":-1,
               "wm":-1,
               "hm":-1
            }
         }
      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"input tap {wm} {hm}"
         }
      },
      "order":0,
      "type":"pattern",
      "preDelay":200,
      "postDelay":2000,
      "maxLoop":10,
      "loop":0,
      "maxTry":10,
      "try":0,
      "timeout":500,
      "next":[
         "tk_follow_end"
      ]
   },
   "tk_follow_end":{
      "name":"HOME",
      "desc":"Go home",
      "trigger":[
         
      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"\"input keyevent 4 & sleep 1; input keyevent 3\""
         }
      },
      "order":0,
      "type":"static",
      "preDelay":1000,
      "postDelay":1000,
      "maxLoop":10,
      "loop":0,
      "maxTry":1,
      "try":0,
      "timeout":5000,
      "next":[
         "tk_follow_ch_open_1"
      ]
   },
