   "tk_x3c_reader":{
      "name":"TK_READ_1",
      "desc":"tiktok read",
      "trigger":[
            {
                "crop": [153, 957, 444, 60],
                "pre":[
                    "if (!accounts.includes(text)&&text!=''){",
                    "   accounts.push(text)",
                    "   result=true",
                    "}"],       
                "result": "result",
                "post":[]
            },
            {
                "crop": [153, 1100, 444, 60],
                "pre":[
                    "if (!accounts.includes(text)&&x==-1&&text!=''){",
                    "   accounts.push(text)",
                    "   x=xm",
                    "   y=ym",
                    "}"
                ],
                "result":"result",
                "post":[]
            },
            {
                "crop": [153, 1260, 444, 60],
                "pre":[
                    "if (!accounts.includes(text)&&x==-1&&text!=''){",
                    "   accounts.push(text)",
                    "   x=xm",
                    "   y=ym",
                    "}",
                    "if(x==-1) result = false"
                ],
                "result":"result",
                "post":[]
            }
      ],
      "command":{
         "action":"adb",
         "devices":"{deviceId}",
         "data":{
            "command":"input tap {x} {y}"
         }
      },
      "order":0,
      "type":"reader",
      "getScreen":true,
      "preDelay":200,
      "postDelay":12000,
      "maxLoop":10,
      "loop":0,
      "maxTry":10,
      "try":0,
      "timeout":5000,
      "next":[
         "tk_x3c_open_apk"
      ]
   },
